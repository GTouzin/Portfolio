    "{\n",
    "    result<- data.frame(movie_name= character(0), actor_2_name= character(0), act_2_mean_gross = numeric(0))\n",
    "\n",
    "    for (i in unique(column)){\n",
    "        df<-movies[actor_2_name==i,]\n",
    "        total<-sum(df$gross_2016,na.rm=TRUE)\n",
    "        \n",
    "        for (j in 1:nrow(df)){\n",
    "            if(nrow(df)==1){\n",
    "                mean<-0\n",
