function [m]= Generation( fdp )
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here
a=length(fdp);
interval=zeros(a-1,3);

for x=1:a-1
    interval(x,1)=fdp(x+1,1);
    interval(x,2)=fdp(x,1);
    interval(x,3)=fdp(x+1,2)-fdp(x,2);    
end
m=interval;
end

